# Set mysql user to new database. Random means randomize value.
[mysql-user]
random = true

# Set mysql user password to new database. Random means randomize value.
[mysql-password]
random = true

# Set mysql user allowed hosts to database.
[mysql-user-hosts]
hosts = 127.0.0.1
hosts = localhost

# Set mysql user privileges. Multiple values allowed.
[mysql-user-privileges]
privileges = ALL

# Set mysql user grant option.
[mysql-grant-option]
value = true

# Set mysql new database name. Random means randomize value.
[database-name]
random = true

# General info of drupal project root and install type.
[install-info]
drupal-root = /Users/tuomas/Sites/www/tivia/tivia-drupal
install-type = template
template-path =  /Users/tuomas/go/src/github.com/tuomasvapaavuori/site_installer/templates/site-templates/template1
sitename = local.tivia-drupal1.fi
sub-directory = local.tivia-drupal1.fi
http-user = _www
http-group = _www
server-config-root = /Users/tuomas/Sites/www/tivia/conf/apache

# Settings for http server config.
[http-server]
type = apache
template = /Users/tuomas/go/src/github.com/tuomasvapaavuori/site_installer/templates/server/apache/default.conf
port = 80

# Settings for ssl server config.
[ssl-server]
type = apache
template = /Users/tuomas/go/src/github.com/tuomasvapaavuori/site_installer/templates/server/apache/ssl.default.conf
port = 443
certificate = /Users/tuomasvapaavuori/go/src/github.com/tuomasvapaavuori/site_installer/templates/server/certs/ssl.crt
key = /Users/tuomasvapaavuori/go/src/github.com/tuomasvapaavuori/site_installer/templates/server/certs/ssl.key
