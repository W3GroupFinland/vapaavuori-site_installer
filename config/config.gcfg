# Settings for application database.
[mysql]
user = hostmaster
password = hostmaster
protocol = tcp
host = 127.0.0.1
port = 3306
dbname = information_schema

# Settings for drush command
[drush]

# Settings for http server
[http-server]
restart = apachectl restart

# Backup directory
[backup]
directory = /tmp

# Hosts file directory
[hosts]
directory = /etc/hosts